    "release:local": "scripts/release.sh cmd=publish",
    "release:ci": "scripts/release.sh cmd=publish CI=true"
  },
  "devDependencies": {
    "@kcinternal/commandline": "0.7.0",
    "@kcinternal/configuration": "0.5.6",
