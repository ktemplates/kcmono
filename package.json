{
  "name": "root",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "root-build",
    "build:ci": "root-build",
    "test": "root-test",
    "test:ci": "root-test",
    "clean": "root-clean",
    "clean:all": "root-clean --all",
    "dev:gwebsite": "scripts/runner.sh type=exec cmd=develop on=gwebsite",
